{
  "name": "hello-world",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap-sass": "^3.4.1",
    "moment": "^2.24.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2",
    "rxjs": "6.5.4"
  },
  "devDependencies": {
    "baqend": "^2.13.0",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "react-scripts": "3.4.1"
  },
  "scripts": {
    "build-css": "node-sass assets/ -o assets/",
    "watch-css": "npm run build-css && node-sass assets/ -o assets/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
